{
  "name": "announce_rj_app",
  "main": "app/_entry.js",
  "version": "3.0.0",
  "scripts": {
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "prebuild": "expo prebuild",
    "build:android": "npm run prebuild && cd android && gradlew.bat assembleRelease",
    "build:ios": "npm run prebuild && cd ios && xcodebuild -workspace announce_rj_app.xcworkspace -scheme announce_rj_app -configuration Release"
  },
  "dependencies": {
    "@expo/vector-icons": "~14.0.4",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^7.0.0",
    "axios": "^1.6.7",
    "expo": "~52.0.47",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "^0.13.3",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.19",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "fs-extra": "^11.3.0",
    "lucide-react-native": "^0.475.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-date-picker": "^4.3.3",
    "react-native-flipper": "^0.273.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-track-player": "^4.1.1",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "expo-dev-client": "~5.0.20",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "typescript": "^5.1.3"
  },
  "expo": {
    "name": "announce_rj_app",
    "slug": "announce_rj_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.subhra.raiseapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
